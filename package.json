{
  "name": "MyAppiumProject",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.0.0",
    "@wdio/appium-service": "^8.39.1",
    "@wdio/browserstack-service": "^8.40.2",
    "@wdio/cli": "^8.39.1",
    "@wdio/concise-reporter": "^9.0.0",
    "@wdio/local-runner": "^8.39.1",
    "@wdio/mocha-framework": "^8.39.0",
    "@wdio/spec-reporter": "^8.39.0",
    "allure-commandline": "^2.30.0",
    "appium": "^2.11.2",
    "appium-uiautomator2-driver": "^3.7.4",
    "appium-xcuitest-driver": "^7.24.9",
    "browserstack-local": "^1.5.5",
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "wdio": "wdio run ./config/wdio.shared.conf.js",
    "ios": "wdio run ./config/wdio.ios.conf.js",
